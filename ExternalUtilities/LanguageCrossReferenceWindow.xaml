<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="ExternalUtilities.LanguageCrossReferenceWindow"
        Title="LanguageCrossReferenceWindow" Loaded="Window_Loaded" >
    <ScrollViewer>
        <StackPanel Orientation="Vertical">
            <Label Content="Warnings"/>
            <TextBox x:Name="WarningsTextBox" Margin="4,0" Background="#FFFBD9D9" FontFamily="Courier New" FontSize="11"/>
            <Label Content="Translations" Background="White"/>
            <TextBox x:Name="TranslationsTextBox"  Margin="4,0" Background="#FFDAF3EE" FontSize="11"/>
        </StackPanel>
    </ScrollViewer>
</Window>
